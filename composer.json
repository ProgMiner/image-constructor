{
    "name": "progminer/image-constructor",
    "description": "Library for object oriented image generation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eridan Domoratskiy",
            "email": "eridan200@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
